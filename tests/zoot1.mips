#MATHIEU STEINBACH Hugo & MOSELLE Marie-Luc

.data

AffichageVrai: .asciiz "vrai"
AffichageFaux: .asciiz "faux"
saut_ligne: .asciiz "\n"

.text

main: 
   #Initialisation de la base des variables :
	move $s7, $sp
	addi $sp, $sp, -16

   #Debut du programme :

   #ecrire 42
	li $v0, 42
	move $a0, $v0
	li $v0, 1
	syscall
	la $a0, saut_ligne
	li $v0, 4
	syscall

   #ecrire faux
	li $v0, 0
	move $a0, $v0
	beq $zero, $a0, Sinon12
	la $a0, AffichageVrai
	li $v0, 4
	syscall
	b FinSi12
	Sinon12:
	la $a0, AffichageFaux
	li $v0, 4
	syscall
	FinSi12:
	la $a0, saut_ligne
	li $v0, 4
	syscall

   #ecrire vrai
	li $v0, 1
	move $a0, $v0
	beq $zero, $a0, Sinon13
	la $a0, AffichageVrai
	li $v0, 4
	syscall
	b FinSi13
	Sinon13:
	la $a0, AffichageFaux
	li $v0, 4
	syscall
	FinSi13:
	la $a0, saut_ligne
	li $v0, 4
	syscall

   #a = 5
	li $v0, 5
	sw $v0, 0($s7)
   #c = a
	lw $v0, 0($s7)

	sw $v0, -8($s7)
   #b = vrai
	li $v0, 1
	sw $v0, -4($s7)
   #d = faux
	li $v0, 0
	sw $v0, -12($s7)
   #ecrire a
	lw $v0, 0($s7)

	move $a0, $v0
	li $v0, 1
	syscall
	la $a0, saut_ligne
	li $v0, 4
	syscall

   #ecrire b
	lw $v0, -4($s7)

	move $a0, $v0
	beq $zero, $a0, Sinon19
	la $a0, AffichageVrai
	li $v0, 4
	syscall
	b FinSi19
	Sinon19:
	la $a0, AffichageFaux
	li $v0, 4
	syscall
	FinSi19:
	la $a0, saut_ligne
	li $v0, 4
	syscall

   #ecrire c
	lw $v0, -8($s7)

	move $a0, $v0
	li $v0, 1
	syscall
	la $a0, saut_ligne
	li $v0, 4
	syscall

   #ecrire d
	lw $v0, -12($s7)

	move $a0, $v0
	beq $zero, $a0, Sinon21
	la $a0, AffichageVrai
	li $v0, 4
	syscall
	b FinSi21
	Sinon21:
	la $a0, AffichageFaux
	li $v0, 4
	syscall
	FinSi21:
	la $a0, saut_ligne
	li $v0, 4
	syscall

	b end

   #Fin du programme :
	end:
	li $v0, 10
	syscall
